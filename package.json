{
  "name": "tv-shows",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "cross-env NITRO_PRESET=cloudflare_pages nuxi build",
    "dev": "wrangler d1 execute tv_shows --local --file=./schema.sql && wrangler pages dev dist --compatibility-date=2023-10-30",
    "postinstall": "nuxi prepare",
    "deploy": "pnpm run build && wrangler pages deploy ./dist",
    "lint:scripts": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "fix:scripts": "eslint --ext .js,.vue --ignore-path .gitignore . --fix",
    "lint:styles": "stylelint \"**/*.{vue,scss}\"",
    "fix:styles": "stylelint \"**/*.{vue,scss}\" --fix"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20231025.0",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@typescript-eslint/parser": "^6.11.0",
    "@vue/reactivity": "^3.3.8",
    "@vue/runtime-core": "^3.3.8",
    "@vue/runtime-dom": "^3.3.8",
    "@vue/shared": "^3.3.8",
    "cross-env": "^7.0.3",
    "eslint": "^7.0.0",
    "eslint-config-galexia": "github:Galexia-Agency/eslint-config",
    "h3": "^1.8.2",
    "nitropack": "^2.7.2",
    "nuxt": "^3.8.1",
    "stylelint": "^15.11.0",
    "stylelint-config-galexia": "github:Galexia-Agency/stylelint-config",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "wrangler": "^3.15.0"
  },
  "browserslist": [
    "last 5 versions and not ie <= 11 and > 0.2% and not dead and not op_mini all"
  ]
}
